    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Example Test",
            "type": "Ruby",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "env": {
                "RUBYLIB": "test"
            },
            "args": ["-v"],
            "useBundler": true,
            "program": "test/util/core_extensions_test.rb"
        },
        {
            "name": "Run Example Demo",
            "type": "Ruby",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "env": {
                "RUBYLIB": "test"
            },
            "args": ["demo/test.md", "-v"],
            "useBundler": true,
            "program": "bin/qed"
        }
    ]
}